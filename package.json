    "ejs": "^3.1.8",
    "express": "^4.18.1",
    "express-validation": "^4.1.0",
    "generate-password": "^1.7.0",
    "helmet": "^5.1.0",
    "husky": "^8.0.1",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "mime-types": "^2.1.35",
    "moment": "^2.29.3",
    "multer": "^1.4.4",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.7.5",
    "sequelize": "^6.20.1",
    "shortid": "^2.2.16",
    "twilio": "^3.77.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^28.1.0",
    "jest-sonar-reporter": "^2.0.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "sequelize-cli": "^6.4.1",
